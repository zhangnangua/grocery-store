<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btn_go_lifecycle_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="GO_LIFECYCLE_ACTIVITY"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/btn_go_viewmodel_timing_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="GO_VIEWMODEL_TIMING_ACTIVITY"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_go_lifecycle_activity"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/btn_go_livedata_timing_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="GO_LIVEDATA_TIMING_ACTIVITY"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_go_viewmodel_timing_activity"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/btn_go_livedata_share_fragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="GO_LIVEDATA_SHARE_FRAGMENT"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_go_livedata_timing_activity"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/btn_go_room_simple_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="GO_ROOM_SIMPLE_DEMO_ACTIVITY"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_go_livedata_share_fragment"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/btn_go_room_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="GO_ROOM_DEMO_ACTIVITY"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_go_room_simple_activity"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/btn_go_simple_databinding_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="GO_SIMPLE_DATA_BINDING_ACTIVITY"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_go_room_activity"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/btn_go_two_way_databinding_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="GO_TWO_WAY_DATA_BINDING_ACTIVITY"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_go_simple_databinding_activity"
        tools:ignore="HardcodedText" />

</androidx.constraintlayout.widget.ConstraintLayout>